  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/macstock10/bevalc-intelligence/issues"
  },
  "homepage": "https://github.com/macstock10/bevalc-intelligence#readme",
  "devDependencies": {
    "@playwright/test": "^1.52.0"
  }
}
